{{#with this as |optionSet|}}
CodeSystem: {{toPascalCase optionSet.name}}CS
Title: "{{{escapeQuotes optionSet.name}}}"
{{#if optionSet.description}}
Description: "{{escapeQuotes optionSet.description}}"
{{/if}}
{{#each optionSet.options as |option|}}
* #"{{option.code}}" "{{escapeQuotes option.name}}"
{{/each}}
{{/with}}